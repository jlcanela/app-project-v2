{
  "name": "@app/frontend",
  "private": true,
  "type": "module",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "typecheck": "tsc --noEmit",
    "lint": "pnpm run eslint && pnpm run stylelint",
    "codegen": "graphql-codegen",
    "eslint": "eslint . --cache",
    "stylelint": "stylelint 'src/**/*.css' --cache",
    "prettier": "prettier --check \"**/*.{ts,tsx}\" \"!src/routeTree.gen.ts\"",
    "prettier:write": "prettier --write \"**/*.{ts,tsx}\" \"!src/routeTree.gen.ts\"",
    "vitest": "vitest run",
    "vitest:watch": "vitest",
    "pretty-test": "pnpm run prettier:write && pnpm test",
    "test": "pnpm run typecheck && pnpm run prettier && pnpm run lint && pnpm run vitest",
    "test:ci": "pnpm run test",
    "storybook": "storybook dev -p 6006",
    "storybook:build": "storybook build"
  },
  "dependencies": {
    "@ant-design/icons": "^6.1.0",
    "@effect-atom/atom-react": "^0.5.0",
    "@gorules/jdm-editor": "^1.51.2",
    "@graphql-typed-document-node/core": "^3.2.0",
    "@mantine/core": "8.3.14",
    "@mantine/dates": "^8.3.14",
    "@mantine/form": "^8.3.14",
    "@mantine/hooks": "8.3.14",
    "@opentelemetry/exporter-trace-otlp-http": "^0.208.0",
    "@opentelemetry/sdk-trace-web": "^2.5.1",
    "@tabler/icons-react": "^3.36.1",
    "@tanstack/react-router": "^1.158.4",
    "@tanstack/react-router-devtools": "^1.158.4",
    "effect": "^3.12.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.37.0",
    "@graphql-codegen/cli": "^5.0.2",
    "@graphql-codegen/typescript": "^4.0.6",
    "@graphql-codegen/typescript-operations": "^4.2.0",
    "@ianvs/prettier-plugin-sort-imports": "^4.7.0",
    "@storybook/addon-themes": "^10.0.0",
    "@storybook/react": "^10.0.0",
    "@storybook/react-vite": "^10.0.0",
    "@tanstack/router-plugin": "^1.158.4",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^25.0.0",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.1",
    "@vitejs/plugin-react": "^5.0.4",
    "eslint": "^9.37.0",
    "eslint-config-mantine": "^4.0.3",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.5",
    "graphql": "^16.8.1",
    "identity-obj-proxy": "^3.0.0",
    "jsdom": "^27.0.0",
    "postcss": "^8.5.6",
    "postcss-preset-mantine": "1.18.0",
    "postcss-simple-vars": "^7.0.1",
    "prettier": "^3.6.2",
    "prop-types": "^15.8.1",
    "storybook": "^10.0.0",
    "stylelint": "^17.0.0",
    "stylelint-config-standard-scss": "^17.0.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.46.0",
    "vite": "npm:rolldown-vite@latest",
